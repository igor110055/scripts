name: BoxJs
desc: http://boxjs.com

http:
  force-http-engine:
    - "boxjs.com:80"
  mitm:
    - "boxjs.com"
  script:
    - match: ^https?:\/\/boxjs\.com
      name: BoxJs
      type: request
      require-body: true
      timeout: 120

script-providers:
  BoxJs:
    url: https://raw.githubusercontent.com/chavyleung/scripts/master/chavy.box.js
    interval: 86400
